<?xml version="1.0"?>
<?xml-stylesheet href="chrome://myn/skin/skin.css" type="text/css"?> 
<!DOCTYPE myn SYSTEM "chrome://myn/locale/translations.dtd">
<overlay id="sample" xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
		<script src="myn.js" />
		
		<menupopup id="menu_ToolsPopup"> 
			<menuitem label="&runMyn;" key="myn-run-key" oncommand="myn.run()"/> 
		</menupopup>
		
		<keyset>
			<key id="myn-run-key" modifiers="accel alt shift" key="L" oncommand="myn.run()"/>
		</keyset>
		
		<statusbar id="status-bar">
			<statusbarpanel id="myn-status-bar-icon" class="statusbarpanel-iconic" src="chrome://linktargetfinder/skin/status-bar.png" tooltiptext="&runMyn;" onclick="myn.run()" />
		</statusbar>
		
		<toolbarpalette id="BrowserToolbarPalette">
			<toolbarbutton id="myn-toolbar-button" label="Mirror your neighbour" tooltiptext="&runMyn;" oncommand="myn.run()"/>
		</toolbarpalette>
</overlay>